{% for tagFormView in formView.tags %}
	<div class="d-flex justify-content-between align-items-center">
		{{ form_widget(tagFormView) }}
		<a href="{{ path('tag_edit', {'id': tagFormView.vars.value}) }}" class="btn btn-link">
			<i class="fas fa-eye"></i>
		</a>
		{{ form_label(tagFormView) }}
	</div>
{% endfor %}
