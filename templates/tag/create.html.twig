{% extends "base.html.twig" %}

{% block title %}
	Création d'un tag
{% endblock %}

{% block body %}
	<h1>Nouveau tag</h1>
	{{ form_start(formView) }}
	{{ form_errors(formView) }}
	<div class="row">
		<div class="col">
			{{ form_row(formView.name) }}
		</div>
	</div>
	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>
		Création
	</button>
	{{ form_end(formView) }}
	{# {{ include('partials/_form.html.twig', {button_text: 'Créer le produit'}) }} #}
{% endblock %}
